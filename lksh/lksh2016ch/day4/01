inline int get(int used, int S) {
	int cur = 0;
	
	int H[31], cH = 0;
	
	for (int i = 0; i < n; i++)
		if ((used & (1 << i)) == 0)
			H[cH++] = T[i];
	
	sort(H, H + cH);
	reverse(H, H + cH);
	
	for (int i = 0; i < n; i += 3) {
		S -= H[i];
		if (i + 1 < n)
			S -= H[i + 1];
		if (i + 2 < n)
			S -= H[i + 2];
		cur += S;
	}	
	return cur;
}

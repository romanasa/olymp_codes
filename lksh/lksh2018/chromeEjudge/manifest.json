{
  "manifest_version": 2,
  "name": "EjudgeAutoSID",
  "version": "1.0",
  "description": "AutoSID to settings",
  "icons": { "16" : "icon.png",
                        "48" : "icon.png",
                        "128" : "icon.png"},
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
          "webRequest",
          "<all_urls>",
          "webNavigation",
          "tabs",
          "cookies"
        ],
  "background": {
    "scripts": ["background.js"]
  }
}